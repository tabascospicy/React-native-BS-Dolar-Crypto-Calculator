{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": "src/",
    
    "resolveJsonModule": true,
    "strict": true
  },
  "paths": {
      "src/*": ["./*"],
    },
  "include": ["src"],
	"exclude": ["node_modules","**/*.spec.ts"],
}

{
  "expo": {
    "name": "Aftim-Calc",
    "slug": "Aftim-Calc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/AFTIM.png",
    "splash": {
      "image": "./src/assets/splash/AftimSplash.png",
      "backgroundColor": "#FCFCFC"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.somossistemas.AftimCalc",
      "versionCode": 5
    },
    "web": {
      "favicon": "./src/assets/icons/favicon.png"
    }
  }
}
